
@{
    ViewBag.Title = "Detail";
}

<div ng-controller="LeadCtrl" ng-init="Editinit()">
    <div class="jumbotron PageHeading">
        <div class="row">
            <div class="col-md-4 col-sm-4 col-lg-4"><h2>Lead Details</h2></div>
            <div class="col-md-4 col-sm-4 col-lg-4" style="padding: 0px; margin: 0px; margin-top: 14px;">
                @*Domain
                    <div class="form-group"  style="display:inline-block;">
                        <select ng-model="Lead.Domain" class="form-control">
                            <option value="Services">Services</option>
                            <option value="Connectivity">Connectivity</option>
                        </select>
                    </div>*@
            </div>
            <div class="col-md-4 col-sm-4 col-lg-4" style=" margin-top: 15px; float: right; text-align: right;">
                Domain
                <div class="form-group" style="display:inline-block;">
                    <input readonly type="text" class="form-control" ng-model="Lead.DomainName" />
                </div>
            </div>

        </div>
    </div>
    <div class="jumbotron PageHeading">
        <div class="text-center">
            <div class="row"> <h2 class="SegmentHeading">General Information</h2></div>
        </div>
        <div class="row">
            <form id="LeadAddForm ">
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Name</label>
                        <input type="text" readonly class="form-control" ng-model="Lead.Name" />
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-md-3">
                        <label>Company</label>
                        <input type="text" readonly class="form-control" ng-model="Lead.CompanyName" />
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-md-3">
                        <label>Contact Person Name</label>
                        <input type="text" readonly class="form-control" ng-model="Lead.ContactPersonName" />
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-3">
                        <label>Contact Person Number</label>
                        <input type="text" readonly class="form-control" ng-model="Lead.ContactPersonNumber" />
                    </div>
                </div>
            </form>
        </div>
        @*<div class="row float-right">
                <button class="btn btn-primary" ng-click="AddLead(Lead)">Add</button>
            </div>*@
    </div>
    <div class="jumbotron PageHeading">
        <div class="row text-center">
            <h2 class="SegmentHeading">Contact Information</h2>
        </div>
        <div class="row">
            <div class="col-md-4 form-group">
                <label>Mode Of Communication</label>
                <input type="text" readonly class="form-control" ng-model="Lead.MOCName" />
            </div>
            <div class="col-md-4 form-group">
                <label>Business Operation Time</label>
                <input type="text" readonly class="form-control" ng-model="Lead.BOTEnum" />
            </div>
            <div class="col-md-4 form-group">
                <label>Phone Number</label>
                <input type="text" readonly class="form-control" ng-model="Lead.Contact" placeholder="033xxxx.." />
            </div>
            <div class="col-md-4 form-group">
                <label>Alternate Number</label>
                <input type="text" readonly class="form-control" ng-model="Lead.CompanyAlternateContact" placeholder="033xxxx.." />
            </div>
            <div class="col-md-4 form-group">
                <label>Email</label>
                <input type="email" readonly class="form-control" ng-model="Lead.Email" placeholder="johndoe@live.com" />
            </div>
            <div class="col-md-4 form-group">
                <label>Website</label>
                <input type="text" readonly class="form-control" ng-model="Lead.Website" placeholder="www.sharptel.com" />
            </div>
            <div class="col-md-4 form-group">
                <label>Area / Territory</label>
                <input type="text" readonly class="form-control" ng-model="Lead.AreaEnum" />

            </div>
            <div class="col-md-4 form-group">
                <label>City</label>
                <input type="text" readonly class="form-control" ng-model="Lead.CityEnum" />
            </div>
            <div class="col-md-4 form-group">
                <label>Address</label>
                <input type="text" readonly ng-model="Lead.Address" class="form-control" />
            </div>

        </div>
    </div>





    <div class="jumbotron PageHeading">
        <div class="row text-center">
            <h2 class="SegmentHeading">Business Infromation</h2>
        </div>
        <div class="row">
            <div class="col-md-4 form-group">
                <label>Number Of Employees</label>
                <input type="text" readonly class="form-control" ng-model="Lead.NOEEnum" />
            </div>
            <div class="col-md-4 form-group">
                <label>Numbe of Branch offices</label>
                <input type="text" readonly ng-model="Lead.NoBracnhOffices" class="form-control" />
            </div>
            <div class="col-md-4 form-group">
                <label>Business Industry</label>
                <input type="text" readonly ng-model="Lead.BusinessIndustryEnum" class="form-control" />
            </div>
            <div class="col-md-4 form-group">
                <label>NTN</label>
                <input type="text" readonly ng-model="Lead.NTN" class="form-control" />
            </div>
            <div class="col-md-4 form-group">
                <label>Currently Using IT Platform</label>
                <input type="text" readonly ng-model="Lead.CurrentItPlatform" class="form-control" />
            </div>
            @*<div class="col-md-4 form-group">
                <label>Future Plan For Extension In IT Service</label>
                <input type="text" ng-model="Lead.FuturePlanOFExtention" class="form-control" />
            </div>*@
            <div class="col-md-4 form-group">
                <label>Currently Used Data/Internet Service</label>
                <input type="text" readonly ng-model="Lead.CUDSEnum" class="form-control" />
            </div>
            <div class="col-md-4 form-group" ng-if="Lead.CUDS == null">
                <span readonly ng-if="Lead.CUDS == null">N/A</span>
            </div>
            <div class="col-md-4 form-group" ng-if="Lead.CUDS != null">
                <span ng-if="Lead.CUDS != '4' ">
                    <label>Select Service</label>
                    <input type="text" readonly ng-model="Lead.CUDSServiceEnum" class="form-control" />
                </span>
                <span ng-if="Lead.CUDS == '4' ">
                    <label>Select Service</label>
                    <input type="text" readonly class="form-control" ng-model="Lead.CUDSOtherService" />
                </span>
            </div>
            <div class="col-md-4 form-group">
                <label>Number Of Links</label>
                <input type="text" readonly class="form-control" ng-model="Lead.NoLinks" />
            </div>
            <div class="col-md-4 form-group">
                <label>Budget</label>
                <input type="text" readonly class="form-control" ng-model="Lead.Budget" />
            </div>
            <div class="col-md-4 form-group">
                <label>Estimated Closing Date</label>
                <input type="datetime" readonly class="form-control" ng-model="Lead.EstimatedClosingDate" />
            </div>
            <div class="col-md-4 form-group">
                <label>Comments</label>
                <input type="text" readonly class="form-control" ng-model="Lead.Comments" />
            </div>
            <div class="col-md-4 form-group" style="padding-bottom: 35px;">
                <label>Is our Existing Customer</label>
                <input type="checkbox" readonly ng-model="Lead.IsEsisting" />
            </div>
            <div class="col-md-4 form-group">
                <label>Ever Tried Our Service Before</label>
                <input type="checkbox" readonly ng-model="Lead.HasTriedOurServie" />
            </div>

        </div>
        <div class="row float-right">
            <a href="/Lead" class="btn btn-danger">Cancel</a>
        </div>

    </div>
</div>